
config globals 'globals'
    option mmx_mask '0x3F00'
    list rt_table_lookup '220'

config interface 'wan2'
    option enabled '1'
    option family 'ipv4'
    option bind_interface 'apclix0'
    list track_ip '119.29.29.29'
    list track_ip '114.114.114.114'
    option reliability '1'
    option count '5'
    option timeout '3'
    option interval '10'
    option down '3'
    option up '3'

config interface 'wan3'
    option enabled '1'
    option family 'ipv4'
    option bind_interface 'apcli0'
    list track_ip '119.29.29.29'
    list track_ip '114.114.114.114'
    option reliability '1'
    option count '5'
    option timeout '3'
    option interval '10'
    option down '3'
    option up '3'

config member 'wan2_primary'
    option interface 'wan2'
    option metric '1'
    option weight '1'

config member 'wan3_backup'
    option interface 'wan3'
    option metric '2'
    option weight '1'

config policy 'primary_backup'
    list use_member 'wan2_primary'
    list use_member 'wan3_backup'

config rule 'default_rule_v4'
    option dest_ip '0.0.0.0/0'
    option use_policy 'primary_backup'
    option family 'ipv4'
    option proto 'all'
