config globals 'globals'
	option mmx_mask '0x3F00'

config interface 'wan2'
	option enabled '1'
	option family 'ipv4'
	list track_ip '119.29.29.29'
	list track_ip '223.5.5.5'
	option track_method 'ping'
	option reliability '1'
	option count '5'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option keep_failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list flush_conntrack 'ifup'
	option force_link '1'

config interface 'wan3'
	option enabled '1'
	option family 'ipv4'
	list track_ip '119.29.29.29'
	list track_ip '223.5.5.5'
	option track_method 'ping'
	option reliability '1'
	option count '5'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option keep_failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	list flush_conntrack 'ifup'
	option force_link '1'

config member 'wan2_primary'
	option interface 'wan2'
	option metric '1'
	option weight '1'

config member 'wan3_backup'
	option interface 'wan3'
	option metric '2'
	option weight '1'

config policy 'primary_backup'
	list use_member 'wan2_primary'
	list use_member 'wan3_backup'
	option last_resort 'unreachable'

config rule 'default_ip_v4'
	option dest_ip '0.0.0.0/0'
	option family 'ipv4'
	option proto 'all'
	option use_policy 'primary_backup'
	option sticky '1'
